<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" targetNamespace="http://it.dpe.igeriv/inforiv/schemas"
	xmlns:inforiv="http://it.dpe.igeriv/inforiv/schemas">
	<xs:element name="EdicolaInforivRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="inforiv:EdicolaInforiv" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EdicolaInforivResponse">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Stato" type="xs:integer" />
				<xs:element name="Messaggio" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="EdicolaInforiv">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="inforiv:CodFiegDl" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="inforiv:CodFiegDl2" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="inforiv:CodFiegDl3" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="inforiv:CodEdicolaDl" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="inforiv:CodEdicolaDl2" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="inforiv:CodEdicolaDl3" minOccurs="0" maxOccurs="1"/>
				<xs:element ref="inforiv:RagioneSociale" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="inforiv:Indirizzo"/>
				<xs:element ref="inforiv:Localita"/>
				<xs:element ref="inforiv:SiglaProvincia"/>
				<xs:element ref="inforiv:Cap"/>
				<xs:element ref="inforiv:PartitaIva"/>
				<xs:element ref="inforiv:CodiceFiscale"/>
				<xs:element ref="inforiv:Telefono"/>
				<xs:element ref="inforiv:Fax"/>
				<xs:element ref="inforiv:Email" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="inforiv:FtpHost" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="inforiv:FtpUser" minOccurs="1" maxOccurs="1"/>
				<xs:element ref="inforiv:FtpPwd" minOccurs="1" maxOccurs="1"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="CodFiegDl" type="xs:integer"/>
	<xs:element name="CodFiegDl2" type="xs:integer"/>
	<xs:element name="CodFiegDl3" type="xs:integer"/>
	<xs:element name="CodEdicolaDl" type="xs:integer"/>
	<xs:element name="CodEdicolaDl2" type="xs:integer"/>
	<xs:element name="CodEdicolaDl3" type="xs:integer"/>
	<xs:element name="RagioneSociale" type="xs:string" />
	<xs:element name="Indirizzo" type="xs:string" />
	<xs:element name="Localita" type="xs:string" />
	<xs:element name="SiglaProvincia" type="xs:string" />
	<xs:element name="Cap" type="xs:string" />
	<xs:element name="PartitaIva" type="xs:string" />
	<xs:element name="CodiceFiscale" type="xs:string" />
	<xs:element name="Telefono" type="xs:string" />
	<xs:element name="Fax" type="xs:string" />
	<xs:element name="Email" type="xs:string" />
	<xs:element name="FtpHost" type="xs:string"/>
	<xs:element name="FtpUser" type="xs:string"/>
	<xs:element name="FtpPwd" type="xs:string"/>
</xs:schema>