<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" targetNamespace="http://it.dpe.igeriv/gdo/ven/schemas"
	xmlns:ven="http://it.dpe.igeriv/gdo/ven/schemas">
	
	<xs:complexType name="Vendita">
		<xs:sequence>
				<xs:element name="barcode" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="idtn" type="xs:integer" minOccurs="1" maxOccurs="1"/>
				<xs:element name="dataOraVendita" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
				<xs:element name="copie" type="xs:integer" minOccurs="1" maxOccurs="1"/>
				<xs:element name="prezzoLordo" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:element name="GdoVenRequest">
		<xs:complexType>
			<xs:sequence>
         		<xs:element name="listVendite" type="ven:Vendita" minOccurs="1" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="GdoVenResponse">
		<xs:complexType>
         <xs:sequence>
         	<xs:element name="dataOra" type="xs:dateTime" minOccurs="1" maxOccurs="1"/>
            <xs:element name="state" type="xs:integer" minOccurs="1" maxOccurs="1"/>
            <xs:element name="listMessages" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>
         </xs:sequence>
      </xs:complexType>
	</xs:element>
		
</xs:schema>
