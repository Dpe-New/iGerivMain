<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified" targetNamespace="http://it.dpe.igeriv/prod/client/ven/schemas"
	xmlns:pcv="http://it.dpe.igeriv/prod/client/ven/schemas">	
	
	<xs:element name="ProdottiClientVenditeRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="coddl" type="xs:integer" minOccurs="1" maxOccurs="1"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="ProdottiClientVenditeResponse">
		<xs:complexType>
         <xs:sequence>
         	<xs:element name="edicola" type="pcv:Edicola" minOccurs="1" maxOccurs="1"/>
            <xs:element name="listProdotti" type="pcv:ProdottoClientVendite" minOccurs="1" maxOccurs="unbounded"/>
            <xs:element name="state" type="xs:integer" minOccurs="1" maxOccurs="1"/>
            <xs:element name="message" type="xs:string" minOccurs="1" maxOccurs="1"/>
         </xs:sequence>
      </xs:complexType>
	</xs:element>
    
    <xs:complexType name="Edicola">
		<xs:sequence>
				<xs:element name="nome" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="localita" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="provincia" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="cap" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="indirizzo" type="xs:string" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>	
	
	<xs:complexType name="ProdottoClientVendite">
		<xs:sequence>
				<xs:element name="barcode" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="idtn" type="xs:integer" minOccurs="1" maxOccurs="1"/>
				<xs:element name="titolo" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="sottotitolo" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="dataUscita" type="xs:date" minOccurs="1" maxOccurs="1"/>
				<xs:element name="numeroCopertina" type="xs:string" minOccurs="1" maxOccurs="1"/>
				<xs:element name="prezzo" type="xs:decimal" minOccurs="1" maxOccurs="1"/>
				<xs:element name="coddl" type="xs:integer" minOccurs="1" maxOccurs="1"/>
				<xs:element name="cpu" type="xs:integer" minOccurs="1" maxOccurs="1"/>
				<xs:element name="ciq" type="xs:integer" minOccurs="0" maxOccurs="1"/>
				<xs:element name="cfq" type="xs:integer" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>		
</xs:schema>
