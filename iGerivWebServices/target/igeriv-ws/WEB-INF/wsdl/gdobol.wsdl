<?xml version="1.0" encoding="UTF-8"?><wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:tns="http://it.dpe.igeriv/gdo/bol/schemas" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:sch="http://it.dpe.igeriv/gdo/bol/schemas" targetNamespace="http://it.dpe.igeriv/gdo/bol/schemas">
  <wsdl:types>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:bol="http://it.dpe.igeriv/gdo/bol/schemas" elementFormDefault="qualified" targetNamespace="http://it.dpe.igeriv/gdo/bol/schemas">
	
	<xs:element name="GdoBollaRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="1" minOccurs="1" name="dataBolla" type="xs:date"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
	<xs:element name="GdoBollaResponse">
		<xs:complexType>
         <xs:sequence>
         	<xs:element maxOccurs="1" minOccurs="1" name="dataOra" type="xs:dateTime"/>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="listProdottiBolla" type="bol:ProdottoBolla"/>
            <!-- 
            FLAG DI STATO DELLA CHIAMATA:
            0 = SUCCESSO
            -1 = ERRORE
            1 = WARNING
             -->
            <xs:element maxOccurs="1" minOccurs="1" name="state" type="xs:integer"/>
            <!--
            MESSAGGIO
            state = 0 => ''
            state = -1 => STACKTRACE DELL'ERRORE
            state = 1 => MESSAGGIO DI STATO (per es. "Credenziali invalide", "Pubblicazione inesistente" o altro)  
             -->
            <xs:element maxOccurs="1" minOccurs="1" name="message" type="xs:string"/>
         </xs:sequence>
      </xs:complexType>
	</xs:element>
    
	<xs:complexType name="ProdottoBolla">
		<xs:sequence>
				<xs:element maxOccurs="1" minOccurs="1" name="barcode" type="xs:string"/>
				<xs:element maxOccurs="1" minOccurs="1" name="idtn" type="xs:integer"/>
				<xs:element maxOccurs="1" minOccurs="1" name="codicePubblicazione" type="xs:integer"/>
				<xs:element maxOccurs="1" minOccurs="0" name="codiceInizio" type="xs:integer"/>
				<!--
	            'Q' = QUOTIDIANO
	            'P' => PERDIODICO  
	             -->
				<xs:element maxOccurs="1" minOccurs="1" name="tipoPubblicazione" type="xs:string"/>
				<xs:element maxOccurs="1" minOccurs="1" name="titolo" type="xs:string"/>
				<xs:element maxOccurs="1" minOccurs="1" name="sottotitolo" type="xs:string"/>
				<xs:element maxOccurs="1" minOccurs="1" name="descrizione" type="xs:string"/>
				<!--
	           		formato ddMMyyyy
	             -->
				<xs:element maxOccurs="1" minOccurs="1" name="dataUscita" type="xs:string"/>
				<xs:element maxOccurs="1" minOccurs="1" name="numeroCopertina" type="xs:string"/>
				<xs:element maxOccurs="1" minOccurs="1" name="prezzoAcquisto" type="xs:decimal"/>
				<xs:element maxOccurs="1" minOccurs="1" name="prezzoLordo" type="xs:decimal"/>
				<xs:element maxOccurs="1" minOccurs="1" name="quantitaConsegnata" type="xs:integer"/>
		</xs:sequence>
	</xs:complexType>		
</xs:schema>
  </wsdl:types>
  <wsdl:message name="GdoBollaResponse">
    <wsdl:part element="tns:GdoBollaResponse" name="GdoBollaResponse">
    </wsdl:part>
  </wsdl:message>
  <wsdl:message name="GdoBollaRequest">
    <wsdl:part element="tns:GdoBollaRequest" name="GdoBollaRequest">
    </wsdl:part>
  </wsdl:message>
  <wsdl:portType name="it.dpe.igeriv/gdo/bol/schemas">
    <wsdl:operation name="GdoBolla">
      <wsdl:input message="tns:GdoBollaRequest" name="GdoBollaRequest">
    </wsdl:input>
      <wsdl:output message="tns:GdoBollaResponse" name="GdoBollaResponse">
    </wsdl:output>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:binding name="it.dpe.igeriv/gdo/bol/schemasSoap11" type="tns:it.dpe.igeriv/gdo/bol/schemas">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
    <wsdl:operation name="GdoBolla">
      <soap:operation soapAction=""/>
      <wsdl:input name="GdoBollaRequest">
        <soap:body use="literal"/>
      </wsdl:input>
      <wsdl:output name="GdoBollaResponse">
        <soap:body use="literal"/>
      </wsdl:output>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="it.dpe.igeriv/gdo/bol/schemasService">
    <wsdl:port binding="tns:it.dpe.igeriv/gdo/bol/schemasSoap11" name="it.dpe.igeriv/gdo/bol/schemasSoap11">
      <soap:address location="http://www.igeriv.it:80/igeriv-ws/ws/"/>
    </wsdl:port>
  </wsdl:service>
</wsdl:definitions>