        <entry key="SA" value-ref="InforivEstrattoContoTotaleImporter"/>
        <entry key="PA" value-ref="InforivNullImporter"/>
        <entry key="MB" value-ref="InforivMessaggiImporter"/>
        <entry key="MR" value-ref="InforivNullImporter"/>
        <entry key="ER" value-ref="InforivNullImporter"/>
        <entry key="TR" value-ref="InforivNullImporter"/>
        <entry key="ME" value-ref="InforivNullImporter"/>
        <entry key="RF" value-ref="InforivNullImporter"/>
        <entry key="VA" value-ref="InforivNullImporter"/>
        <entry key="ED" value-ref="InforivAnagraficaEditoreImporter"/>
        <entry key="VN" value-ref="InforivVenditeImporter"/>
    </util:map>
    
    <!-- Questa mappa contiene gli oggetti BO di tipo Exporter che eseguono l'esportazione per ogni tipo record specifico -->
    <util:map id="mapExporters" map-class="java.util.LinkedHashMap">
        <entry key="ME" value-ref="InforivMancanzeEccedenzeExporter"/>
        <entry key="RI" value-ref="InforivResaDichiarataExporter"/>
        <entry key="BC" value-ref="InforivTotaleBollaConsegnaAccertataExporter"/>
        <entry key="TR" value-ref="InforivTotaleBollaResaEdicolaExporter"/>
        <entry key="RF" value-ref="InforivRichiestaRifornimentoExporter"/>
        <entry key="VA" value-ref="InforivVariazioniServizioExporter"/>
        <entry key="VE" value-ref="InforivVenditeExporter"/>
    </util:map>

</beans>